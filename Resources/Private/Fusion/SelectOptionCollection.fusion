prototype(Neos.Form.Builder:SelectOptionCollection) {
    @class = 'Neos\\Form\\Builder\\Fusion\\SelectOptionCollectionImplementation'

    prependOptionLabel = ${null}
    prependOptionValue = ${null}

    items = null
    itemName = 'item'
    itemRenderer = Neos.Fusion:DataStructure {
        value = ${q(item).property("value")}
        label = ${q(item).property("label")}
    }
}
